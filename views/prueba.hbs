{{#> blank}}
  {{#*inline "content"}}

    <div class="wrapper-page">

      <div class="account-bg">
            <div class="card-box mb-0">
                <div class="text-center m-t-20">
                    <a href="index.html" class="logo">
                        <i class="zmdi zmdi-group-work icon-c-logo"></i>
                        <span>Uplon</span>
                    </a>
                </div>
                <div class="m-t-10 p-20">
                    <div class="row">
                        <div class="col-12 text-center">
                            <h6 class="text-muted text-uppercase m-b-0 m-t-0">prueba</h6>
                        </div>
                    </div>
                    <form class="m-t-20" action="/post/prueba" method="POST">

                        <div class="form-group row">
                            <div class="col-12">
                                <input class="form-control" type="text" required="" placeholder="slug" name="slug">
                            </div>
                        </div>

                        <div class="form-group row">
                            <div class="col-12">
                                <input class="form-control" type="text" required="" placeholder="title" name="title">
                            </div>
                        </div>
                         <div class="form-group row">
                            <div class="col-12">
                                <input class="form-control" type="text" required="" placeholder="excerpt" name="excerpt">
                            </div>
                        </div>

                        <div class="form-group row">
                            <div class="col-12">
                                <input class="form-control" type="text" required="" placeholder="content" name="content">
                            </div>
                        </div>
                         <div class="form-group row">
                            <div class="col-12">
                                <input class="form-control" type="text" required="" placeholder="category" name="category">
                            </div>
                        </div>

                        <div class="form-group row">
                            <div class="col-12">
                                <input class="form-control" type="text" required="" placeholder="tags" name="tags[]">
                            </div>
                        </div>

                        <div class="form-group text-center row m-t-10">
                            <div class="col-12">
                                <button class="btn btn-success btn-block waves-effect waves-light" type="submit">probar</button>
                            </div>
                        </div>

                    </form>

                </div>

                <div class="clearfix"></div>
            </div>
      </div>
      <!-- end card-box-->

  

    </div>
    <!-- end wrapper page -->
  {{/inline}}
{{/blank}}